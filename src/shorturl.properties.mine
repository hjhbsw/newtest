# c3p0 pool. we have 2 pools: read and write
shorturl.c3p0.driverClass=com.mysql.jdbc.Driver
shorturl.c3p0.minPoolSize=10
shorturl.c3p0.maxPoolSize=50
shorturl.c3p0.idleConnectionTestPeriod=20
