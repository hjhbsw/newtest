shorturl.c3p0.preferredTestQuery=SELECT 1
shorturl.c3p0.breakAfterAcquireFailure=false
shorturl.c3p0.checkoutTimeout=6000
shorturl.c3p0.acquireRetryAttempts=30
shorturl.c3p0.acquireRetryDelay=1000

# time out, milliseconds
