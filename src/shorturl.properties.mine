# id值的前缀 
shorturl.idc = 266666

# turn on/off rpc
shorturl.rpc = off
shorturl.rpc.port = 9008

shorturl.shorthost = u.cntv.cn
#请求中长链接的最大数量
shorturl.params.maxurls = 20
#请求中短链接的最大数量
shorturl.params.maxkeys = 100

#长链接的最小有效长度
shorturl.params.minurllen = 7
#长链接的最大有效长度
shorturl.params.maxurllen = 1024

shorturl.base62.seed = vPh7zZwA2LyU4bGq5tcVfIMxJi6XaSoK9CNp0OWljYTHQ8REnmu31BrdgeDkFs

shorturl.memcache = off
#shorturl.memcache.servers = yf109:11211
shorturl.memcache.servers = 10.87.96.71:11211
# 24 * 3600
shorturl.memcache.timeout = 86400
# clear all caches when update
shorturl.memcache.cleancache = yf96:8082;yf97:8082;tc63:8082;tc64:8082
shorturl.memcache.cleanmainsite = true



#--需要修改为短链接服务器地址
shorturl.shorturl.prefix = u.cntv.cn



# flush after write
shorturl.response.flushafterwrite = false

# for all video and music ext craw
shorturl.httpclient.fast.ThreadPool=250
shorturl.httpclient.fast.ConnectTimeout=1000
shorturl.httpclient.fast.Timeout=1000
shorturl.httpclient.fast.MaxRetries=1
shorturl.httpclient.fast.MaxRedirects=1
shorturl.httpclient.fast.MaxConnectionsPerAddress=20
# for i.t and safe.i.t
shorturl.httpclient.safe.ThreadPool=150
shorturl.httpclient.safe.ConnectTimeout=1000
shorturl.httpclient.safe.Timeout=1000
shorturl.httpclient.safe.MaxRetries=1
shorturl.httpclient.safe.MaxRedirects=1
shorturl.httpclient.safe.MaxConnectionsPerAddress=100
# for repaire
shorturl.httpclient.slow.ThreadPool=50
shorturl.httpclient.slow.ConnectTimeout=5000
shorturl.httpclient.slow.Timeout=10000
shorturl.httpclient.slow.MaxRetries=3
shorturl.httpclient.slow.MaxRedirects=1
shorturl.httpclient.slow.MaxConnectionsPerAddress=5
# for all tinyurl redirct
shorturl.httpclient.redirect.ThreadPool=100
shorturl.httpclient.redirect.ConnectTimeout=1000
shorturl.httpclient.redirect.Timeout=1000
shorturl.httpclient.redirect.MaxRetries=1
shorturl.httpclient.redirect.MaxRedirects=1
shorturl.httpclient.redirect.MaxConnectionsPerAddress=5
# for proxy client
shorturl.httpclient.proxy=10.81.11.65:13128


